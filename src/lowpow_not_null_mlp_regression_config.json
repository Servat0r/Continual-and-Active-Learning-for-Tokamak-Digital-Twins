{
  "general": {
    "task": "regression",
    "dtype": "float32",
    "num_campaigns": 10,
    "train_mb_size": 512,
    "eval_mb_size": 1024,
    "train_epochs": 200
  },
  "dataset": {
    "pow_type": "lowpow",
    "cluster_type": "Ip_Pin_based",
    "dataset_type": "not_null",
    "input_columns": [
      "ane", "ate", "x", "q", "smag", "alpha",
      "ani1", "ati0", "normni1", "zeff", "lognustar"
    ],
    "output_columns": ["efe", "efi", "pfe", "pfi"],
    "input_size": 11,
    "output_size": 4,
    "normalize_inputs": true,
    "normalize_outputs": false
  },
  "architecture": {
    "name": "saved",
    "model_folder": "lowpow_mlp_1024_2_efe_efi_pfe_pfi_8runs",
    "model_name": "model",
    "model_class_name": "MLP",
    "parameters": {
      "input_size": 11,
      "output_size": 4,
      "hidden_size": 1024,
      "hidden_layers": 2,
      "drop_rate": 0.5
    }
  },
  "loss": {
    "name": "MSE",
    "parameters": {
      "reduction":  "mean"
    }
  },
  "optimizer": {
    "name": "Adam",
    "parameters": {
      "lr": 2e-3,
      "weight_decay": 1e-4
    }
  },
  "scheduler": {
    "name": "ReduceLROnPlateau",
    "parameters": {
      "metric": "val_loss",
      "first_exp_only": false,
      "factor": 0.25,
      "patience": 10
    }
  },
  "strategy": [
    {
      "name": "EWC",
      "extra_log_folder": "Lambda 10",
      "parameters": {
        "ewc_lambda": 10.0,
        "mode": "separate"
      }
    },
    {
      "name": "EWCReplay",
      "extra_log_folder": "Lambda 10 Buffer 200",
      "parameters": {
        "mode": "separate",
        "ewc_lambda": 10.0,
        "mem_size": 200
      }
    },
    {
      "name": "EWCReplay",
      "extra_log_folder": "Lambda 10 Buffer 1000",
      "parameters": {
        "mode": "separate",
        "ewc_lambda": 10.0,
        "mem_size": 1000
      }
    },
    {
      "name": "GEM",
      "extra_log_folder": "Patterns 200",
      "parameters": {
        "patterns_per_exp": 200
      }
    },
    {
      "name": "GEMReplay",
      "extra_log_folder": "Patterns 200 Buffer 200",
      "parameters": {
        "patterns_per_exp": 200,
        "mem_size": 200
      }
    },
    {
      "name": "JointTraining",
      "parameters": {
        "eval_every": 0
      }
    }
  ],
  "early_stopping": {
    "patience": 50,
    "delta": 0.1,
    "val_stream_name": "eval_stream",
    "min_epochs": 100
  },
  "validation_stream": {
    "val_stream": "eval_stream"
  },
  "start_model_saving": {
    "save_model": false,
    "saved_model_folder": "lowpow_mlp_1024_2_efe_efi_pfe_pfi_8runs",
    "saved_model_name": "model",
    "add_timestamp": false
  }
}